<Window1@BoxLayout>
    orientation: "vertical"

    ActionBar:
        ActionView:

            ActionPrevious:
                title: 'Want to take a picture?'
                with_previous: False

    BoxLayout:
        Button:
            id: button1
            text: 'Yes'
            on_press: app.root.change_disp2(), app.root.send_selected_key("y")

    BoxLayout:
        Button:
            id: button2
            text: 'No'
            on_press: app.root.change_disp3(), app.root.send_selected_key("n")
